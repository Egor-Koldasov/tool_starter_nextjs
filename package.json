{
  "name": "nextjs-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev & yarn ts:watch",
    "build": "next build",
    "heroku-postbuild": "NEXT_PUBLIC_API_HOST=tool-starter-api.herokuapp.com npm run build",
    "start": "next start",
    "rebuild": "docker-compose stop nextjs && docker-compose up -d --build nextjs",
    "codegen": "graphql-codegen --config codegen.yml",
    "ts": "tsc --noEmit --incremental --preserveWatchOutput",
    "ts:watch": "yarn ts --watch",
    "test": "docker-compose run --rm nextjs npm run test_in",
    "test_in": "node --trace-warnings --async-stack-traces node_modules/.bin/jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "next": "^11.1.2",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "scheduler": "^0.20.2",
    "styled-components": "^5.3.3",
    "use-context-selector": "^1.3.9",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/loadable__component": "^5.13.3",
    "@types/node": "^16.11.7",
    "@types/ramda": "^0.27.46",
    "@types/react": "^17.0.2",
    "@types/react-loadable": "^5.5.4",
    "@types/styled-components": "^5.1.15",
    "@types/webpack-env": "^1.16.3",
    "axios-mock-adapter": "^1.20.0",
    "babel-jest": "^27.4.2",
    "babel-plugin-styled-components": "^2.0.2",
    "husky": "^7.0.4",
    "jest": "^27.4.2",
    "jest-css-modules-transform": "^4.3.0",
    "next-page-tester": "^0.30.0",
    "next-router-mock": "^0.6.3",
    "sass": "^1.32.8",
    "ts-node": "^10.4.0",
    "type-fest": "^2.5.2",
    "typescript": "^4.5.2"
  },
  "resolutions": {
    "graphql": "^15.7.2"
  }
}
